import NavJummahTimes from './components/NavJummahTimes';
import Welcome from './components/Welcome';
---

<html lang="en">
	<head>
		<meta charset="utf-8" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="viewport" content="width=device-width" />
		<meta name="generator" content={Astro.generator} />
		<title>Astro</title>
	</head>
	<body>
		<Nav client:load></Nav>
		<div class=" hidden lg:block">
			<NavPrayerTimes client:load></NavPrayerTimes>
		</div>
		
		<NavJummahTimes client:load></NavJummahTimes>
		<Welcome client:load ></Welcome>
	</body>
</html>
